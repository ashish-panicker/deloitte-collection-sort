package deloitte.day2;

public class Main {

	public static void main(String[] args) {
		
		Student s1 = new Student(1,"Ashish", 
				"ashish@gmail.com");
		
		Student s2 = new Student();
		s2.setId(2);
		s2.setName("rahul");
		s2.setEmail("rahul@mail.com");
		
		Student s3 = new Student(1,"Ashish", 
				"ashish@gmail.com");;
		
//		System.out.println(s1);
	
		if(s1.equals(s3)) {
			System.out.println("Students are same.");
		}else {
			System.err.println("Students are not same.");
		}
		
		String [] names = {"Data 1", "Data 2", "Data 3"};
		
		System.out.println(names.length);
		
		for (int i = 0; i < names.length; i++) {
			System.out.println(names[i]);
		}
		System.err.println("Enhanced for..loop");
		
		for(String str : names) {
			System.out.println(str);
		}
		
		add(1,2,3);
		add(1,2,3,4,5);
		
	}
	
	public static void add(int... arr) {
		
	}

}
