package com.deloitte.india;

public class Test {

	public int var;
	
	private double pi = 3.14;
	
	public Test(){
		
		System.out.println("var = " + var);
		var = 100;		
		
		pi= 4.33;
	}
	
	public int add(int a, int b) {
		return a + b;
	}
	
	public long add(int a , long b) {
		return a + b;
	}
	
	public long add(long a, int b) {
		return a + b;
	}
	
	public double area(double r) {
		return pi * (r * r);
	}
		
}
